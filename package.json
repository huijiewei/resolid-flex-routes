{
  "name": "@resolid/flex-routes",
  "version": "1.0.1",
  "type": "module",
  "sideEffects": false,
  "description": "Resolid React Router 灵活路由",
  "author": "Huijie Wei",
  "license": "MIT",
  "keywords": ["resolid", "react-router", "file-system-routes"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": ["dist"],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check": "biome check --write .",
    "check-exports": "attw --pack . --profile esm-only",
    "build": "tsup",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "minimatch": "^10.0.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.1",
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@react-router/dev": "^7.0.2",
    "@resolid/config": "^2.1.2",
    "@types/node": "^20.17.9",
    "lefthook": "^1.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@react-router/dev": "^7.0.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "homepage": "https://www.resolid.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/resolid-flex-routes.git"
  },
  "pnpm": {
    "overrides": {
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "isarray": "npm:@nolyfill/isarray@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1"
    }
  }
}
